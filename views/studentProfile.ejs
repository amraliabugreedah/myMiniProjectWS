<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Student profile</title>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
    <!-- <script type="text/javascript" src="jquery-1.8.3.js"></script> -->
    <!-- <script src="./myJQueryScripts.js"></script> -->
  </head>
  <body>
<div class="container">
  <br>
  <div class = "row">
     <!-- <form class="" id="studentProfile" action="/getProfile" method="get"> -->

       <div class="col-sm-6">
         <% if(profile.pic != ""){%>
            <img src="<%= profile.pic %>" alt="Mountain View" style="width:304px;height:228px;">
         <% }%>
       </div>

       <div class="col-sm-6">

         <b><%= profile.name %></b>

       </div>

       <div>

       </div>
<!--
     </form> -->
   </div>

<br>
<br>
   <div class = "row">
      <form class="" id="studentProfileForm" action="/addWork" method="post">
        <div class = "row">
          <div class="col-sm-1">
            <label for="screenshot">screenshot</label>
            </div>
            <div class="col-sm-11">
              <input type="file" name="screenshot" placeholder="screenshot">
            </div>
      </div>
        <br>
        <div class = "row">
        <div class="col-sm-1">
          <label for="link">Link</label>
        </div>
          <div class="col-sm-11">
          <input type="text" name="link" placeholder="Link">
        </div>
      </div>
        <br>
        <div class = "row">
          <div class="col-sm-1">
            <label for="codeRepo">Code Repo</label>
          </div>
            <div class="col-sm-10">
            <input type="text" name="codeRepo" placeholder="codeRepo">
          </div>
      </div>
        <br>

        <div>
          <button type="submit" name="button">AddWork</button>

        </div>

      </form>
<br>
      <form class="" id="viewWorkForm" action="/viewWork" method="get">

        <div>
          <button type="submit" name="button">View my work</button>

        </div>

      </form>



    </div>

</div>

<script type="text/javascript">

</script>

  </body>
</html>
